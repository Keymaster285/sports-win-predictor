        "np.random.seed(42)\n",
        "\n",
        "num_games = 300\n",
        "\n",
        "data = {\n",
        "    'home_game': np.random.randint(0, 2, num_games),\n",
        "    'avg_points_scored': np.random.normal(110, 10, num_games),\n",
        "    'avg_points_allowed': np.random.normal(108, 10, num_games),\n",
        "    'win_percentage':np.random.uniform(0.3, 0.7, num_games),\n",
        "    'rest_days': np.random.randint(0, 5, num_games)\n",
        "}\n",
        "\n",
        "df = pd.DataFrame(data)\n",
        "#df.info()\n",
        "df.describe()\n",
        "df.head(7)"
